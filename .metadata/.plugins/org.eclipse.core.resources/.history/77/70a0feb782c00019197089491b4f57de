package br.com.elaborata.exercicios;

/**
 * @author Leo
 *
 */
public class Aluno {
	private Double media;
	private final Integer numeroDeNotas = 3;

	public void setMedia() {
		Teclado teclado = new Teclado();
		
		for (int i = 0; i < numeroDeNotas; i++) {
			
			teclado.leNota();
			
		}
		
	}
	
}
