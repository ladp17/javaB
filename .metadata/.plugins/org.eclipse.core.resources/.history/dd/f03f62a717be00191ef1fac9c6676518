package br.com.elaborata.exercicios;

/**
 * @author Leo
 *
 */
public class CalculaTabuada {
	
	public void calculaTabuada(Integer numero, Integer fim){
		
		for (int i = 1; i <= fim; i++) {
			Integer resultado;
			
			resultado = numero * i;
			
			System.out.println(numero + " * " + i + " = " + resultado );
			
		}
		
	}

}
