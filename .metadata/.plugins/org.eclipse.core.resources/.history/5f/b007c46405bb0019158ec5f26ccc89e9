package br.com.elaborata.lista3.ex2;

import java.util.ArrayList;

/**
 * @author Leo
 *
 */

public class Menu {
	Teclado teclado = new Teclado();
	ArrayList<Item> menu = new ArrayList<Item>();
	
	public void menu() {
		
		do {
			
			for (Item item : menu) {
				
				teclado
				
			}
			
		} while (true);
		
	}
		
	
	
}
