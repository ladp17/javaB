package br.com.elaborata.exercicios;

/**
 * @author Leo
 *
 */
public class ImprimeEntre {
	
	public void ordenaNumeros(Integer n1, Integer n2) {
		Integer aux;
		
		if (n1 > n2) {
			
			aux = n1;
			n1 = n2;
			n2 = aux;
			
		}
		
	}
	
	public void imprimeEntre(Integer n1, Integer n2) {
		
		ordenaNumeros(n1, n2);
		
		for (int i = n1; i < n2 - 1;){
			
			System.out.println(++i);
			
		} 
		
	}

}
