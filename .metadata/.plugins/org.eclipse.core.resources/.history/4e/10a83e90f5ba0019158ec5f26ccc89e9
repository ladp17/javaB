package br.com.elaborata.ex1;

/**
 * @author Leo
 *
 */
public class ImprimeDivisiveis {

	/**
	 * @param args
	 */
	
	public Boolean ehDivisivel(Integer divisor, Integer dividendo){
		
		if ((dividendo.intValue() % divisor.intValue()) == 0 ) {
			
			return true;
			
		} else {
			
			return false;

		}
		
	}	
	
	public static void main(String[] args) {
	

	}

}
