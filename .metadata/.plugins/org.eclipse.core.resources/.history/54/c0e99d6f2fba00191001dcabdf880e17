public class Bollean {
	
	private Boolean bool;
	
	public Boolean imprimeVdd(Boolean bool){
		
		System.out.println(this.bool + " and " + bool + " = " + (this.bool && bool));
		System.out.println(this.bool + " or " + bool + " = " + (this.bool || bool));
		System.out.println(this.bool + " xor " + bool + " = " + (this.bool ^ bool));
		
	} 

